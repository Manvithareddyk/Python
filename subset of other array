def is_subarray(arr, sub):
    for i in range(len(arr) - len(sub) + 1):  # slide window
        if arr[i:i+len(sub)] == sub:          # check if slice matches
            return True
    return False

# Example:
arr = list(map(int, input("Enter main array: ").split()))
sub = list(map(int, input("Enter subarray: ").split()))

if is_subarray(arr, sub):
    print("Yes, it is a subarray.")
else:
    print("No, it is not a subarray.")
