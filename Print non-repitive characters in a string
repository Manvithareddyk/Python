text = input("Enter a string: ")
freq = {}

# Step 1: Count frequency of each character
for ch in text:
    if ch in freq:
        freq[ch] += 1
    else:
        freq[ch] = 1

# Step 2: Print only non-repeating characters
for ch in text:  # not set(text) â€” to preserve order
    if freq[ch] == 1:
        print(ch, end="")
